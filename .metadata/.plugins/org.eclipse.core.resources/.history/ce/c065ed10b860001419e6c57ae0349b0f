package Interfaz;

public class Habitacion {
	int numeroHabitacion, edificio, nivel, camas, precio;
	String sucursal, tipo;
	Habitacion siguiente=null;
	
	public Habitacion(int tipo){
		switch (tipo){
		case 1:
			precio = 200;
			break;
		case 2:
			precio = 250;
			break;
		case 3:
			precio = 379;
			break;
		default:
			precio = 0;
			break;
		}
	}
	
}
